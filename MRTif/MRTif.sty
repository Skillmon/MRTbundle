    \MRTif@create@TandF@twoargs{#1Nn}%
    \long\expandafter\edef\csname MRTif#1nNTF\endcsname##1##2%
      {%
        \unexpanded{\expandafter\MRTif@nN@helper\expandafter}%
        {##2}{##1}%
